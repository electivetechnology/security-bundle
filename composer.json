{
    "name": "electivegroup/security-bundle",
    "type": "symfony-bundle",
    "description": "Elective Security Bundle",
    "keywords": ["elective", "symfony", "security", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kris Rybak",
            "email": "kris@elective.io"
        }
    ],
    "require": {
        "php": "^7.1.13",
        "lexik/jwt-authentication-bundle": "^2.6",
        "symfony/http-foundation": "^5",
        "symfony/security-bundle": "^5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3",
        "symfony/phpunit-bridge": "^4.3|^5.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Elective\\SecurityBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Elective\\SecurityBundle\\Tests\\": "tests/" }
    },
    "minimum-stability": "stable"
}
